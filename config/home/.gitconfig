[branch]                                                                                                              
	autosetuprebase = always
[user]
	email = lu.yuan@altran-beyondsoft.com
	name = yuanlu 80000014
[color]
	ui = true
[core]
	editor = vi
[alias]
##############################################
# add alias settings
##############################################
	ad = add 
	uad = add -u
	aad = add -A
	iad = add -i
	pad = add --patch

##############################################
# branch alias setings
##############################################
	br = branch
	vbr = branch -v
	mbr = branch -m
	dbr = branch -d
	rbr = branch -r
	mgbr = branch --merged
	nmbr = branch --no-merge

##############################################
# bisect alias setings
##############################################
	bs = bisect
	sbs = bisect start
	gbs = bisect good
	bbs = bisect bad 
	lbs = bisect log 
	rpbs = bisect replay
	bbco = checkout bisect/bad
	rbs = bisect reset

##############################################
# blame alias setings
##############################################
	bm = blame
	lbm = blame -L

##############################################
# clone alias setings
##############################################
	cl = clone
	bcl = clone --bare
	mcl = clone --mirror

##############################################
# checkout alias setings
##############################################
	co = checkout
	bco = checkout -b
	tco = checkout --track
	tbco = checkout --track -b

##############################################
# commit alias setings
##############################################
	ci = commit
	mci = commit -m
	amci = commit -a -m
	smci = commit -s -m
	cci = commit -C
	scci = commit -s -C
	amci = commit --amend -m
	asmci = commit --amend -s -m
	acci = commit --amend -C
	ascci = commit --amend -s -C
	aemci = commit --allow-empty -m

##############################################
# cherry-pick alias setings
##############################################
	cp = cherry-pick

##############################################
# cat-file alias setings
##############################################
	cf = cat-file
	pcf = cat-file -p
	tcf = cat-file -t

##############################################
# diff alias setings
##############################################
	df = diff
	cdf = diff --cached
	hdf = diff HEAD
	sdf = diff --stat
	cdf = diff --check

##############################################
# descibe alias setings
##############################################
	db = describe

##############################################
# format-patch alias setings
##############################################
	fp = format-patch

##############################################
# mergtool alias setings
##############################################
	mt = mergetool

##############################################
# fetch alias setings
##############################################
	ft = fetch
	nft = fetch --no-tags

##############################################
# config alias setings
##############################################
	gcfg = config --global
	egcfg = config --global -e
	ecfg = config -e
	lcfg = config --list
	hcfg = config --help

##############################################
# log alias setings
##############################################
	lg = log
	glg = log --graph
	olg = log --oneline
	dlg = log --decorate
	rlg = log --raw
	slg = log --stat
	plg = log -p
	polg = log --pretty=oneline
	pflg = log --pretty=fuller
	oglg = log --oneline --graph
	alg = log --pretty=fuller --graph --raw --decorate
	flg=  log --pretty=format:"%h - %an, %ar : %s"
	gflg=  log --pretty=format:"%h - %an, %ar : %s" --graph

##############################################
# ls-files alias setings
##############################################
	lf = ls-files
	slf = ls-files -s

##############################################
# ls-remote alias setings
##############################################
	lr = ls-remote
	hlr = ls-remote --heads
	tlr = ls-remote --tags

##############################################
# tag alias setings
##############################################
	mtag = tag -m
	atag = tag -a
	dtag = tag -d
	ttag = tag --tags
	ntag = tag -n1
	tdb = describe --tags

##############################################
# merge alias setings
##############################################
	mg = merge
	cmg = merge --continue
	smg = merge --skip
	amg = merge --abort

##############################################
# name-rev alias setings
##############################################
	nr = name-rev
	tnr = name-rev --tags

##############################################
# rebase alias setings
##############################################
	rb = rebase
	orb = rebase --onto
	irb = rebase -i

##############################################
# reset alias setings
##############################################
	rst = reset
	hrst = reset --hard
	srst = reset --soft

##############################################
# reflog alias setings
##############################################
	rl = reflog

##############################################
# rev-parse alias setings
##############################################
	rp = rev-parse
	gd = rev-parse --git-dir
	grd = rev-parse --show-cdup
	brp = rev-parse --symbolic --branches       
	trp = rev-parse --symbolic --tags

##############################################
# rev-list alias setings
##############################################
	rls = rev-list
	orls = rev-list --oneline

##############################################
# remote alias setings
##############################################
	rt = remote
	art = remote add
	rrt = remote rm
	vrt = remote -v
	surt = remote set-url
	psurt = remote --push set-url
	udrt = remote update    
	rnrt = remote rename
	srt = remote show

##############################################
# pull alias setings
##############################################
	rpull = pull --rebase

##############################################
# format-patch am alias setings
##############################################
	sfp = format-patch -s
	ram = am --resolved
	sam = am --skip
	aam = am --abort

##############################################
