##############################################
	ss = stash
	ass = stash apply
	pss = stash pop
	piss = stash pop --index
	lss = stash list
	css = stash clear
[merge]
	tool = extMerge
[mergetool "extMerge"]
	cmd = extMerge \"$BASE\" \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
[mergetool]
	trustExitCode = false
[diff]
	external = extDiff
